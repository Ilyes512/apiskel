{
    "name": "ilyes512/apiskel",
    "description": "A packages that helps you create api's.",
    "keywords": [
        "apiskel",
        "laravel",
        "lumen",
        "api"
    ],
    "homepage": "https://github.com/ilyes512/apiskel",
    "license": "MIT",
    "authors": [
        {
            "name": "Ilyes Ahidar",
            "email": "ilyes.ahidar@gmail.com",
            "homepage": "http://ilyes.nl",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "league/fractal": "~0.13",
        "illuminate/contracts": "~5.1"
    },
    "require-dev": {
        "crysalead/kahlan": "~1.2",
        "illuminate/support": "~5.1"
    },
    "autoload": {
        "psr-4": {
            "Ilyes512\\Apiskel\\": "src",
            "Ilyes512\\Spec\\": "spec"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ilyes512\\Apiskel\\Test\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}